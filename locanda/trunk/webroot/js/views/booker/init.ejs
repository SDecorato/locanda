<h2>bookers</h2>
<table cellspacing='0px'>
	<thead>
	<tr>
		<% for(var attr in Locanda.Models.Booker.attributes){%>
			<% if(attr == 'id') continue;%>
			<th><%= attr%> </th>	 
		<%}%>
		<th>Options</th>
	</tr>
	</thead>
	<tbody>
		<%= $.View('//locanda/views/booker/list',{bookers: bookers})%>
	</tbody>
</table>
<h2>New booker</h2>
<form>
	<% for(var attr in Locanda.Models.Booker.attributes){ %>
		<% if(attr == 'id') continue;%>
		<div>
			<label><%= attr %> </label><br />
			<input type="text" value="" name="<%= attr %>"/>
		</div>
	<%}%>
	<input type='submit' value='Create'/>
</form>